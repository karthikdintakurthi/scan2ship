{
  "name": "vanitha-logistics-app",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf node_modules/.prisma && prisma generate && next build",
    "build:local": "rm -rf node_modules/.prisma && prisma generate && node scripts/cache-bust.js && next build",
    "start": "next start",
    "prestart": "node scripts/validate-environment.js && node scripts/validate-security-config.js",
    "postinstall": "rm -rf node_modules/.prisma && prisma generate",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:security": "jest --testPathPattern=security --coverage",
    "test:auth": "jest --testPathPattern=auth --coverage",
    "test:api": "jest --testPathPattern=api --coverage",
    "test:api:auth": "jest --testPathPattern=api/__tests__/auth --coverage",
    "db:backup": "./scripts/backup-db.sh",
    "db:migrate:safe": "./scripts/safe-migrate.sh",
    "db:migrate:dev": "npm run db:backup && npx prisma migrate dev",
    "db:migrate:deploy": "npm run db:backup && npx prisma migrate deploy",
    "db:status": "npx prisma migrate status",
    "db:studio": "prisma studio",
    "db:copy-prod": "node scripts/copy-prod-data-prisma.js",
    "db:migrate-prod-to-local": "./scripts/migrate-prod-to-local.sh",
    "db:sync-versions": "node scripts/sync-db-versions.js",
    "db:add-a5-to-prod": "node scripts/add-a5-field-to-prod.js",
    "test:api:admin": "jest --testPathPattern=api/__tests__/admin --coverage",
    "test:api:orders": "jest --testPathPattern=api/__tests__/orders --coverage",
    "test:api:webhooks": "jest --testPathPattern=api/__tests__/webhooks --coverage",
    "test:api:shopify": "jest --testPathPattern=api/__tests__/shopify --coverage",
    "test:api:all": "node scripts/run-api-tests.js",
    "test:api:critical": "jest --testPathPattern='(auth|webhooks)' --coverage",
    "test:api:high": "jest --testPathPattern='(admin|orders|shopify)' --coverage",
    "coverage:audit": "node scripts/test-coverage-audit.js",
    "coverage:report": "npm run test:coverage && npm run coverage:audit",
    "coverage:api": "npm run test:api:all && npm run coverage:audit",
    "setup-db": "node scripts/setup-db.js",
    "seed": "node scripts/seed-data.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "db:seed": "node scripts/seed-data.js",
    "setup-railway": "node scripts/setup-railway-db.js",
    "railway": "node scripts/railway-db.js",
    "validate-env": "node scripts/validate-environment.js",
    "validate-security": "node scripts/validate-security-config.js",
    "migrate:prod-to-qa": "node scripts/migrate-prod-to-qa.js",
    "migrate:prod-to-qa-prisma": "node scripts/migrate-prod-to-qa-prisma.js",
    "migrate:prod-to-qa-direct": "node scripts/migrate-prod-to-qa-direct.js",
    "migrate:prod-to-qa-corrected": "node scripts/migrate-prod-to-qa-corrected.js",
    "fix:sequences": "node scripts/fix-sequences-after-migration.js"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@types/lodash": "^4.17.20",
    "@types/qrcode": "^1.5.5",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "exceljs": "^4.4.0",
    "express-rate-limit": "^8.0.1",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "limiter": "^3.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.542.0",
    "mysql2": "^3.14.3",
    "next": "15.5.0",
    "node-fetch": "^3.3.2",
    "pdfkit": "^0.17.1",
    "pg": "^8.16.3",
    "prisma": "^6.14.0",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sonner": "^2.0.7",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/exceljs": "^0.5.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/pdfkit": "^0.17.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/xlsx": "^0.0.35",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^30.1.2",
    "c8": "^10.1.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "nyc": "^17.1.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5"
  }
}
